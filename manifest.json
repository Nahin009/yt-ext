{
  "manifest_version": 3,
  "name": "Video Loop Segment",
  "version": "1.0",
  "description": "Loops a video between 50s and 70s.",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
